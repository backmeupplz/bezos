{
  "name": "bezos",
  "version": "1.0.0",
  "description": "Bot for the Telegram group Bezos Giveaway",
  "main": "dist/app.js",
  "scripts": {
    "distribute": "yarn build-ts && node dist/app.js",
    "develop": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold\" \"yarn watch-ts\" \"yarn watch-js\"",
    "build-ts": "tsc --skipLibCheck",
    "watch-ts": "tsc -w --skipLibCheck",
    "watch-js": "nodemon dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/backmeupplz/bezos.git"
  },
  "keywords": [
    "giveaway",
    "telegram",
    "bot"
  ],
  "author": "backmeupplz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/backmeupplz/bezos/issues"
  },
  "homepage": "https://github.com/backmeupplz/bezos#readme",
  "dependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/lodash": "^4.14.112",
    "@types/mongoose": "^5.2.0",
    "@types/node": "^10.5.2",
    "@types/node-schedule": "^1.2.2",
    "@types/reflect-metadata": "^0.1.0",
    "@types/web3": "^1.0.0",
    "concurrently": "^3.6.0",
    "dotenv": "^6.0.0",
    "ethereum-regex": "^1.1.3",
    "lodash": "^4.17.10",
    "mongoose": "^5.2.4",
    "node-schedule": "^1.3.0",
    "nodemon": "^1.18.2",
    "reflect-metadata": "^0.1.12",
    "telegraf": "^3.21.2",
    "tslint": "^5.11.0",
    "typegoose": "^5.2.1",
    "typescript": "^2.9.2",
    "web3": "^1.0.0-beta.34"
  }
}
